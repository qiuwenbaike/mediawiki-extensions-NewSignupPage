{
	"root": true,
	"extends": [
		"wikimedia/client-common",
		"wikimedia/language/es2022",
		"wikimedia/mediawiki"
	],
	"env": {
		"browser": true
	},
	"globals": {
		"exports": true
	},
	"rules": {
		"space-in-parens": 0,
		"mediawiki/msg-doc": "off"
	},
	"parserOptions": {
		"ecmaVersion": 11,
		"sourceType": "module"
	}
}
